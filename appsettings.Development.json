{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.AspNetCore": "Trace"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:80"
      },
      "HttpsInlineCertFile": {
        "Url": "https://*:443"
      }
    },
    "Certificates": {
      "Default": {
        "Path": "/etc/certs/beryozavv.com.pfx",
        "Password": "$CREDENTIAL_PLACEHOLDER$"
      }
    }
  }
}
